journal = {Tsinghua Science and Technology},
keywords = {Clustering algorithms,Communities,Complex networks,Image edge detection,Inference algorithms,Partitioning algorithms,community detection,latent Dirichlet allocation,link partition,overlapping community},
number = {4},
pages = {387 -- 397},
title = {{LBLP: link-clustering-based approach for overlapping community detection}},
url = {http://ieeexplore.ieee.org/ielx7/5971803/6574669/06574677.pdf?tp={\&}arnumber=6574677{\&}isnumber=6574669},
volume = {18},
