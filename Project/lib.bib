journal = {Proceedings of the 1st international conference on Information-centric networking - INC '14},
keywords = {cache networks,information-centric networks,network optimization,spatio-temporal locality of reference},
pages = {47--56},
title = {{Understanding optimal caching and opportunistic caching at "the edge" of information-centric networks}},
url = {http://doi.acm.org/10.1145/2660129.2660143$\backslash$nhttp://dl.acm.org/citation.cfm?doid=2660129.2660143},
