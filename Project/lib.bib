keywords = {Accuracy,Application identification,Communities,Complex network,Detection algorithms,Internet,Internet service provider,Internet traffic characterization,Internet traffic identification,LPA,Label propagation,Streaming media,Traffic analysis,community detection,complex networks,flow graph,flow graphs,label propagation algorithm,linear time community detection algorithm,public network,telecommunication traffic},
