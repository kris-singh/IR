@article{Caarls2016,
abstract = {Content delivery, such as video streaming, is one of the most prevalent Internet applications. Although very popular, the continuous growth of such applications poses novel performance and scalability challenges. Information-centric networks put content at the center, and propose novel solutions to such challenges but also pose new questions on the interface between caching and routing. In this paper, building on top of Q-routing we propose a caching strategy, namely Q-caching, which leverages information that is already collected by the routing algorithm. Q-caching promotes content diversity in the network, reducing the load at custodians and average download times for clients. In stylized topologies, we show that the gains of Q-caching against state-of-the-art algorithms are significant. We then consider the RNP topology, and show that Q-caching performance is more flexible while competitive when compared against existing algorithms.},
archivePrefix = {arXiv},
arxivId = {1512.08469},
author = {Caarls, Wouter and Hargreaves, Eduardo and Menasch, Daniel S},
eprint = {1512.08469},
file = {:home/kris/Desktop/NE/Q-cahing.pdf:pdf},
journal = {Annals os SBRC 2016},
title = {{Q-caching : an integrated reinforcement-learning approach for caching and routing in information-centric networks}},
year = {2016}
}
@article{Ericsson2000,
abstract = {Driving pattern, i.e. the speed profiles of vehicles, was studied{\$}\backslash{\$}nin connection with variables in the driver-car-environment system.{\$}\backslash{\$}nData were collected using five measuring cars that were driven by{\$}\backslash{\$}n29 randomly chosen families for two weeks each. The cars were equipped{\$}\backslash{\$}nwith data-logging devices that enabled studies of the speed and acceleration{\$}\backslash{\$}npatterns of the vehicles as well as engine speed and gear changing.{\$}\backslash{\$}nFor connection to external conditions co-ordinates for positions{\$}\backslash{\$}nwere registered with global positioning system (GPS) receivers. The{\$}\backslash{\$}nGPS co-ordinates were matched to a digitised map to which detailed{\$}\backslash{\$}nstreet parameters, such as street function, speed limit, width, and{\$}\backslash{\$}ntraffic flow had been attributed. A descriptive analysis of driving{\$}\backslash{\$}npatterns on 21 street types was accomplished. A large set of driving{\$}\backslash{\$}npattern measures including speed, acceleration, power use, engine{\$}\backslash{\$}nspeed, and gear changing behaviour are reported for different street{\$}\backslash{\$}ntypes. Further, a cause effect model for the variation of driving{\$}\backslash{\$}npatterns was estimated. The model included effects of driver characteristics,{\$}\backslash{\$}ncar performance and street environment as well as some important{\$}\backslash{\$}ninteractions between variables. The model was found to predict the{\$}\backslash{\$}nvariation of speed with acceptable explanatory power. For other driving{\$}\backslash{\$}npattern measures significant effects were estimated for street type{\$}\backslash{\$}nas well as driver variables. However, the explanatory power was low;{\$}\backslash{\$}nthe reasons for this are discussed, and bases for new model structures{\$}\backslash{\$}nare outlined.},
